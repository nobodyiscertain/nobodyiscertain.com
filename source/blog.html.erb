 <div class="container narrow">
   <div class="blog__posts">
     <h1>From the Blog</h1>
     <hr />
     <ul class="blog__post-list">
       <% blog.articles.each do |article| %>
         <li >
           <%= link_to article.title, article.url %>
           <p class="description"><%= article.data.description %></p>
         </li>
       <% end %>
     </ul>
   </div>
 </div>
